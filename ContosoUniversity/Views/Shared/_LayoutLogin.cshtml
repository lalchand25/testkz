<body>
    @using ContosoUniversity.Models;
    @Html.Partial("header")


    @RenderBody()


    <div class="container-fluid" style="background: #ad103b; margin: 60px 0 0 0; padding: 30px 0;">
        <div class="container">
            <div class="row">
                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                    <div class="widget_text">
                        <img src="~/images/logo-light.png">

                        <br>
                        <br>

                        <p style="color: #fff;">
                          Your one stop destination to a satisfying lunch
                        </p>


                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">

                    <div class="widget_text">

                        <h6 style="color: #fff; font-size: 18px;">Head Office</h6>
                        <ul style="margin: 0 0 0 -40px; float: left; list-style: none;">
                            <li>
                                <p style="color: #fff;">Lunchpacker.ca</p>
                            </li>
                            <li>
                                <p style="color: #fff;">
                                    <i class="icon-home"></i><span class="strong">Address:  </span>9294, 116 Street,  Delta BC   V4C 5W9
                                </p>
                            </li>

                            <li>
                                <p style="color: #fff;"><i class="icon-phone"></i><span class="strong">Cell: </span>778 997 0025 </p>
                            </li>


                        </ul>
                    </div>

                </div>
               <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                    <div class="widget_text">

                        <h6 style="color: #fff; font-size: 18px;">&nbsp;</h6>
                        <ul style="margin: 0 0 0 -40px; float: left; list-style: none;">
                            <li>
                              
                                <p style="color: #fff;"><i class="office"></i><span class="strong">Cell: </span>778 997 0025</p>
                            </li>
                            <li>
                                <p style="color: #fff;"><i class="fax"></i><span class="strong">Office: </span> 778 897 0171 </p>
                            </li>
                            <li>
                                <p style="color: #fff;"><i class="icon-mail"></i><span class="strong">Email: </span>info@lunchpacker.ca</p>
                            </li>


                        </ul>
                    </div>
                </div>

               <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                    <div class="widget_text">

                        <h6 style="color: #fff; font-size: 18px;">Contact Us</h6>
                        <ul style="margin: 0 0 0 -40px; float: left; list-style: none;">
                            <form name="htmlform" method="post" action="contact.php">
                                <li>
                                    <input name="first_name" type="text" placeholder="Enter Your Name" style="width: 200px; border-radius: 3px; border: none; margin: 0 0 3px 0; padding: 2px 5px; font-size: 12px;" /></li>
                                <li>
                                    <input name="email" type="text" placeholder="Enter Your Email" style="width: 200px; border-radius: 3px; border: none; margin: 0 0 3px 0; padding: 2px 5px; font-size: 12px" /></li>
                                <li>
                                    <input name="telephone" type="text" placeholder="Contact No." style="width: 200px; border-radius: 3px; border: none; margin: 0 0 3px 0; padding: 2px 5px; font-size: 12px" /></li>
                                <li>
                                    <textarea name="comments" placeholder="Enter Your Comment" style="width: 200px; border-radius: 3px; border: none; margin: 0 0 3px 0; padding: 2px 5px; font-size: 12px"></textarea></li>
                                <li>
                                    <input class="submit" value="Submit" type="submit" style="width: 120px;"></li>
                            </form>
                        </ul>
                    </div>
                </div>
            </div>

        </div>
    </div>

    <div class="container">

        <div class="row" style="margin: 15px 0 10px 0">
            <div class="col-lg-12">
                <p style="text-align: center;">Copyright @@lunchpacker.ca 2015. All Rights Reserved.</p>
            </div>
        </div>

    </div>





    @Scripts.Render("~/bundles/otf")
    @RenderSection("scripts", false)


