<!--HEADER START-->
<header>
    <!--TOP STRIP START-->
    <div class="top-strip">
        <div class="container">
            <div class="account">
                <ul>
                    @if (Session["pmsuserid"] != null)
                    {

                        <li><a>Hi @Session["pmsusername"]</a></li>
                        <li><a href="/login/LogOff">Sign Out</a></li>
                        <li>
                            <div class="dropdown">
                                <a class="dropdown-toggle" id="account" role="button" data-toggle="dropdown" data-target="#" href="#">
                                    My Account <b class="caret"></b>
                                </a>
                                <ul class="dropdown-menu" role="menu" aria-labelledby="account">
                                    <li><a href="/Profile/studentProfile">Profile</a></li>
                                    <li><a href="/profile/edit">Edit Profile</a></li>
                                    @*<li><a href="/mytraining">Booked Courses</a></li>*@

                                </ul>
                            </div>
                        </li>



                    }
                    else
                    {
                        <li><a href="/register/signin/?cattype=3">Are You a Teacher or Publisher?</a></li>
                        <li><a href="/login/logon">Sign In</a></li>


                        @*
                            <li><a href="/register/signin">Sign Up</a></li>*@

                    }
                    <!--   -->
                </ul>
            </div>
            <!--ACCOUNT SECTION START-->
        </div>
    </div>
    <!--TOP STRIP END-->
    <!--NAVIGATION START-->
    <div class="navigation-bar">
        <div class="container">
            <div class="logo">
                <a href="~/home/index">
                    <img src="~/images/site-logo.png" alt="">
                </a>
            </div>
            <div class="navigation">
                <div class="navbar">
                    <div class="navbar-inner">
                        <div class="container">
                            <button data-target=".nav-collapse" data-toggle="collapse" class="btn btn-navbar" type="button">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                            <div class="nav-collapse collapse">
                                @*    // admin 8, stu 9, parent2 , teach 3*@
                                <ul>



                                    @if (Session["pmsuserid"] != null)
                                    {
                                        if (Convert.ToInt32(Session["cateid"]) == 9) // stu
                                        {
                                            <li><a href="/reports/certificates">Certificate</a></li>

                                            @* <li><a href="/testFinal/Index">Final Exam </a></li> *@

                                            <li><a href="/exercisenew/finalexm">Final Exam </a></li>

                                            <li><a href="/mytraining">My Courses</a></li>

                                        }

                                        if (Convert.ToInt32(Session["cateid"]) == 3) // teacher
                                        {
                                            <li><a href="/Profile/index">Left Menu</a></li>
                                            <li><a href="#">Students</a></li>
                                            <li><a href="/Profile/IndexTeacher">Help </a></li>
                                        }

                                        @*


                                            <li><a href="/exercisenew/assessment">Assessment </a> </li> *@
                                    }
                                    else
                                    {
                                        <li><a href="~/home/index">Home</a></li>
                                        <li><a href="~/home/aboutNew">About us</a></li>
                                        <li><a href="~/home/howitworks">How it Works</a></li>
                                        <li><a href="~/course/course">Courses</a></li>
                                        <li><a href="~/home/faq">FAQs</a></li>
                                        <li><a href="~/home/contactnew">Contact Us</a></li>
                                    }



                                    @*
                                        <li><a href="main-admin.html">Admin Dashboard</a></li>
                                    *@
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--NAVIGATION END-->
</header>
<!--HEADER END-->
