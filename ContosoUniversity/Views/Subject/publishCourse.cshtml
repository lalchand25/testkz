@model PagedList.IPagedList<OLProject.Models.tb_SubjectMaster>
@using PagedList;
@using PagedList.Mvc;
@using OLProject.Models;
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/adminsecurity.cshtml";
}

<script type="text/javascript">
    function UpdateCatList() {
        destElement = document.getElementById("MainCategoryID");
        id = destElement.value
        var url = '/ProductMaster/subCatlist/' + id;

        $("#tasklistResult").load(url);
    }

</script>
<script type="text/javascript">
    function UpdateCatListAB() {
        destElement = document.getElementById("CategoryID");
        id = destElement.value
        var url = '/ProductMaster/sub22Catlist/' + id;

        $("#divSubcate").load(url);
    }

</script>
<script type="text/javascript" src="../../Jquery_Date/jquery-1.5.1.js"></script>
<script type="text/javascript" src="../../Jquery_Date/jquery.ui.core.js"></script>
<script type="text/javascript" src="../../Jquery_Date/jquery.ui.widget.js"></script>
<script type="text/javascript" src="../../Jquery_Date/jquery.ui.datepicker.js"></script>
<link type="text/css" href="../../Jquery_Date/themes/base/jquery.ui.all.css" rel="stylesheet" />
<h2>Course List </h2>

@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)
   


    <div class="table_div">
       <table class="table table-striped table-bordered table-hover" id="dataTables-example">
            <thead>
                <tr>
                    <th>&nbsp;&nbsp;&nbsp;
                    </th>
                    <th>Image
                    </th>
                    <th>
                        @Html.ActionLink("Name", "Index", new { sortOrder = ViewBag.NameParm })
                    </th>
                    <th>Publish
                    </th>

                </tr>
            </thead>
            <tbody>
                @{int Catid = 0; int MainID = 0; int SubCatID = 0;

                  kzonlineEntities db = new kzonlineEntities();
            
                }
                @foreach (var item in Model)
                {
                    <tr>
                        <td></td>
                        <td>
                            <img src="../../uploads/@item.SubjectImge"  width="50" height="50" />
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SubjectName)
                        </td>
                        <td>
                            @if (item.PublishAdmin == true)
                            {
                                <input type='checkbox' checked='checked' name='selectedobjects' value='@item.SubjectId'> 
                            }
                            else
                            {
                                <input type='checkbox' name='selectedobjects' value='@item.SubjectId'> 
                            }
                        </td>

                    </tr>
                }
            </tbody>
        </table>

    </div>
    <input class="button" value="Submit" type="submit" style="">
}
