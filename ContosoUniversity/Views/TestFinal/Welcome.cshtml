@model OLProject.Models.tb_QuizTestMaster
@{ Layout = "~/Views/Shared/adminpopup.cshtml";
}
@using OLProject.Models;

@Session["projectmetatags"]


<h2> @Html.Raw(Model.TestDesc) </h2>
<table id="resultForm:result_table" class="table table-striped table-bordered table-hover form2">


    <tr>
        <td style="width: 22%;">
            <div class="display-label"><strong>Course Name:</strong></div>
        </td>
        <td style="width: 40%;">
            <div class="display-field">@Html.Raw(ViewData["SubjectName"])</div>
        </td>
        <td style="width: 38%;">&nbsp;</td>
    </tr>


    <tr>
        <td>
            <div class="display-label"><strong>Passing Score:</strong></div>
        </td>
        <td>
            <div class="display-field">@Html.Raw(ViewData["passingScore"])</div>
        </td>
        <td>&nbsp;</td>
    </tr>
    <tr>
        <td>
            <div class="display-label"><strong>Total Question:</strong></div>
        </td>
        <td>
            <div class="display-field">@Html.Raw(ViewData["TotalQuestoin"])</div>
        </td>
        <td>&nbsp;</td>
    </tr>
    <tr>
        <td>
            <div class="display-label"><strong>Time Duration:</strong></div>
        </td>
        <td>
            <div class="display-field">@Html.Raw(ViewData["TimeDuration"])</div>
        </td>
        <td>&nbsp;</td>
    </tr>
    <tr>
        <td>&nbsp;</td>
        <td>@Html.Raw(ViewData["startbutton"]) </td>
        <td>&nbsp;</td>
    </tr>
</table>

