@model IEnumerable<OLProject.Models.tb_ModuleMaster>
@{ Layout = "~/Views/Shared/adminsecurity.cshtml";
}
@Session["projectmetatags"]
<h2>Edit Menu Here @Session["menutaskname"]</h2>
<div class="panel-body">
<div class="dataTable_wrapper">
<div style="margin:0 0 10px 0;">
<a href="/modules/create" id='contract' runat='server'>
<button class="btn btn-success" type="button"><i class="fa fa-plus"></i>Create</button></a>
</div>	
<table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                    <thead>
   
    <tr>

        <th>Item Name</th>
  <th>Status</th>
      <th>Order</th>
	  <th>Edit</th>
	  <th>Delete</th>
    </tr>
	</thead>
                                    <tbody>

    
    @foreach (var item in Model)
    { 
    
        <tr>

            <td>
                @item.ModuleName
            </td>
            <td>Open
            </td>
            <td>
                @item.Displayorderno
            </td>

            <td align="center" width="5%">
                <a href="@Url.Action("Edit", "modules", new { id = item.ModuleId })">
                    <button class="btn btn-warning" type="button"><i class="fa fa-edit"></i>Edit</button></a>
            </td>
            <td align="center" width="5%">
                <a href="@Url.Action("Delete", "modules", new { id = item.ModuleId })">
                    <button class="btn btn-danger" type="button"><i class="fa fa-trash"></i>Delete</button></a>
            </td>
        </tr>
    
    }
   
 </tbody>
</table>
</div>
</div>


