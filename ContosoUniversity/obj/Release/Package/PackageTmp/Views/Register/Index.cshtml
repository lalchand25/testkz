<script type="text/javascript">
    $(document).ready(function () {
        $("#accordion").accordion();
    });

    $(function () {
        $("#tabs").tabs();
    });

    $("#someParentOfA").delegate("#someA", "click", function () {

    });

    $.fx.speeds._default = 500;
    $(function () {
        $("#dialog").dialog({
            autoOpen: false,
            show: "blind",
            hide: "explode",
            height: 320,
            width: 700,
            modal: true

        });
            @Html.Raw(ViewData["script"])

              $("#opener").click(function (id) {
                  LoadData(id);
                  $("#dialog").dialog("open");


                  return false;
              });
          });


          function UpdateStateList() {
              destElement = document.getElementById("CountryID");
              id = destElement.value
              var url = '/city/statelist/' + id;

              $("#tasklistResult").load(url);
          }

          function UpdateDistList() {
              destElement = document.getElementById("StateID");
              id = destElement.value
              var url = '/city/distlist/' + id;
              $("#tasklistResult1").load(url);
          }

          function UpdateTaskList(id) {
              var div1 = document.getElementById("div11");
              var div2 = document.getElementById("div21");
              var div3 = document.getElementById("div31");
              var div4 = document.getElementById("div41");

              div1.style.display = 'none';
              div2.style.display = 'none';
              div3.style.display = 'none';
              div4.style.display = 'none';

              if (id == 1) {
                  div1.style.display = 'block';
              }

              if (id == 2) {
                  div2.style.display = 'block';
              }

              if (id == 3) {
                  div3.style.display = 'block';
              }
              if (id == 4) {
                  div4.style.display = 'block';
              }


          }

          $(function () {

              $("#dialog-confirm").dialog({
                  resizable: false,
                  height: 140,
                  modal: true,
                  buttons: {
                      "Yes": function () {
                          //Function call to commit to database
                      },
                      "No": function () {
                          $(this).dialog("close");
                      }
                  }
              });
          });


</script>
